var n1 4 endvar
var n2 4 endvar
1 n1 !

macro times 46 endmacro

macro printValue // index value -- index
    // 'index : value'
    over 1 +
    print
    " : " print
    print cr
endmacro

times
0 while over over swap < do
    n1 @ n2 @ +
    n2 @
    over n2 !
    n1 !

    printValue

    1 +
endwhile 

