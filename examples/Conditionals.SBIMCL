/*

    INFO
    Testing Conditionals

    == Equal
    != Not Equal
    > Greater Than
    < Less Than
    >= Greater Than or equal to
    <= Less than or equal to
*/

macro space 32 . endmacro
macro a 21 endmacro
macro b 20 endmacro

a b == print space
a b != print space
a b >  print space
a b <  print space
a b >= print space
a b <= print space
cr

if a b >= then
    69 print cr
endif

if a b != then
    420 print cr
endif
