/*
A Basic program to show of the basic Math Operations and show a simple program
*/

var A int
var B int

input cast(int) A swap !
input cast(int) B swap !

func printOperator char int --> define
    A print
    ' ' print
    swap print
    ' ' print
    B print
    " = " print
    print cr
endfunc

'+' A B +       printOperator
'-' A B -       printOperator
'*' A B *       printOperator
'/' A B /       printOperator
'%' A B mod     printOperator
