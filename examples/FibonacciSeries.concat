mem n1 4 endmem
mem n2 4 endmem
n1 1 !

macro times 46 endmacro

func printValue int int --> int define // index value -- index
    // 'index : value'
    over 1 +
    print
    " : " print
    print cr
endfunc

times
0 while over over swap < do
    n1 @ n2 @ +
    n2 @
    over n2 swap !
    n1 swap !

    printValue

    1 +
endwhile 

